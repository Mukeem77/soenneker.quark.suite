
@namespace Soenneker.Quark
@inherits Soenneker.Quark.Element

<Div Class="navbar-start" @attributes="BuildAttributes()" >
    @ChildContent
</Div>

@code {
	public override string? ThemeKey { get; set; } = "BarStart";

    protected override Dictionary<string, object> BuildAttributes()
    {
        var attributes = base.BuildAttributes();

        AppendToClassAttr(attributes, "navbar-start");

        return attributes;
    }

}
