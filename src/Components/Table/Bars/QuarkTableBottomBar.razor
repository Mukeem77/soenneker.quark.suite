
@inherits Element

<Div Class="quark-table-bottombar-wrapper" @attributes="BuildAttributes()">
    <Div Class="@($"quark-table-bottombar {LayoutClass}")">
        @ChildContent
    </Div>
</Div>

@code {
    public override string? ThemeKey { get; set; } = "QuarkTableBottomBar";

    [Parameter]
    public string LayoutClass { get; set; } = string.Empty;
}
